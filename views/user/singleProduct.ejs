<script>
    const stars = document.querySelectorAll(".star");
    const rating = document.getElementById("rating");
    const reviewText = document.getElementById("review");
    const submitBtn = document.getElementById("submit");
    const reviewsContainer = document.getElementById("reviews");

    const dbrating = document.getElementById('editReview').getAttribute('data-rating');
    const review = document.getElementById('editReview').getAttribute('data-review');

    if (review && dbrating) {
        console.log('hi')
        console.log(review, typeof dbrating)
        stars.forEach((s, index) => {
            if (index < parseInt(dbrating)) {
                s.classList.add(getStarColorClass(parseInt(dbrating)));
            }
        });

        reviewText.textContent = review;
        rating.innerHTML = dbrating;
    }

    stars.forEach((star) => {
        star.addEventListener("click", () => {
            const value = parseInt(star.getAttribute("data-value"));
            rating.innerText = value;

            // Remove all existing classes from stars
            stars.forEach((s) => s.classList.remove("one",
                "two",
                "three",
                "four",
                "five"));

            // Add the appropriate class to 
            // each star based on the selected star's value
            stars.forEach((s, index) => {
                if (index < value) {
                    s.classList.add(getStarColorClass(value));
                }
            });

            // Remove "selected" class from all stars
            stars.forEach((s) => s.classList.remove("selected"));
            // Add "selected" class to the clicked star
            star.classList.add("selected");
        });
    });

    submitBtn.addEventListener("click", () => {
        const review = reviewText.value;
        const userRating = parseInt(rating.innerText);

        if (!userRating || !review) {


            return;
        }
        const productId = document.getElementById('productId').getAttribute('data-productId');
        const data = { productId: productId, rating: userRating, ureview: review }
        console.log(data);
        $.ajax({
            type: 'POST',
            url: '/addReview',
            data: JSON.stringify(data),
            contentType: 'application/json',
            success: (response) => {

                if (response.success) {
                    window.location.reload();
                }

            }
        })
    });

    function getStarColorClass(value) {
        switch (value) {
            case 1:
                return "one";
            case 2:
                return "two";
            case 3:
                return "three";
            case 4:
                return "four";
            case 5:
                return "five";
            default:
                return "";
        }
    }

</script>

<script>
    $(document).ready(function () {
    // Function to handle the cancellation request submission
    $('#cancel').on('click', function () {
        const orderId = $('#cancelDetails').data('orderid');
        const productId = $('#cancelDetails').data('productid');
        const index = $('#cancelDetails').data('index');
        const reason = $('#cancelReason').val();

        // Assuming you're using AJAX to send the request
        $.ajax({
            url: '/order-cancel', // Your cancel order endpoint
            method: 'POST',
            data: {
                orderId: orderId,
                productId: productId,
                index: index,
                reason: reason
            },
            success: function (response) {
                // Assuming response.success is true if the request was successful
                if (response.success) {
                    $('#orderCancelation').modal('hide'); // Hide the modal
                    $('#orderCancelationSent').modal('show'); // Show confirmation message
                } else {
                    // Handle error case
                    alert('Failed to send cancellation request.');
                }
            },
            error: function () {
                // Handle error case
                alert('Failed to send cancellation request.');
            }
        });
    });

    // Function to handle the return request submission
    $('#return').on('click', function () {
        const orderId = $('#cancelDetails').data('orderid');
        const productId = $('#cancelDetails').data('productid');
        const index = $('#cancelDetails').data('index');
        const reason = $('#returnReason').val();

        // Assuming you're using AJAX to send the request
        $.ajax({
            url: '/product-return', // Your return product endpoint
            method: 'POST',
            data: {
                orderId: orderId,
                productId: productId,
                index: index,
                reason: reason
            },
            success: function (response) {
                // Assuming response.success is true if the request was successful
                if (response.success) {
                    $('#returnProduct').modal('hide'); // Hide the modal
                    $('#orderCancelationSent').modal('show'); // Show confirmation message
                } else {
                    // Handle error case
                    alert('Failed to send return request.');
                }
            },
            error: function () {
                // Handle error case
                alert('Failed to send return request.');
            }
        });
    });
});
