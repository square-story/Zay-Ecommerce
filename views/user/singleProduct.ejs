    <style>
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }

        a {
            text-decoration: none;

        }

        .status-checkboxes {
            margin-top: .5rem;

        }

        .status-checkboxes .form-check {
            display: flex;
            align-items: center;
            margin-bottom: 1rem;
        }

        .status-checkboxes .form-check-input {
            margin-top: 0;
            margin-right: 5px;
            /* Adjust spacing between checkbox and label */
        }

        .status-checkboxes .form-check-label {
            margin-bottom: 0;
        }

        .progress-bar {
            display: flex;
            align-items: center;
            justify-content: space-between;
            width: 70%;
            margin: 50px auto;
        }

        .step {
            width: 20px;
            height: 20px;
            border: 2px solid #ddd;
            border-radius: 50%;
            display: flex;
            align-items: center;
            justify-content: center;
            font-size: 16px;
        }

        .step.active {
            background-color: #26a541;
            color: white;
            border-color: #26a541;
        }

        .line {
            flex-grow: 1;
            height: 3px;
            background-color: #ddd;
        }


        .container1 {
            background-color: white;
            padding: 20px;
            border-radius: 8px;
            box-shadow: 0 0 10px rgba(0, 0, 0, 0.2);
            text-align: center;
            width: 400px;
        }

        h1 {
            font-size: 40px;
            margin: 0;
        }

        .rating {
            font-size: 20px;
            margin: 10px 0;
        }

        .stars {
            font-size: 30px;
            margin: 10px 0;
        }

        .star {
            cursor: pointer;
            margin: 0 5px;
        }

        .one {
            color: rgb(255, 0, 0);
        }

        .two {
            color: rgb(255, 106, 0);
        }

        .three {
            color: rgb(251, 255, 120);
        }

        .four {
            color: rgb(255, 255, 0);
        }

        .five {
            color: rgb(24, 159, 14);
        }

        textarea {
            width: 90%;
            padding: 10px;
            border: 1px solid #ccc;
            border-radius: 4px;
        }

        button {
            background-color: #007BFF;
            color: white;
            padding: 10px 20px;
            border: none;
            border-radius: 4px;
            cursor: pointer;
        }

        button:hover {
            background-color: #0056b3;
        }

        .reviews {
            margin-top: 20px;
            text-align: left;
        }

        .review {
            border: 1px solid #ccc;
            border-radius: 4px;
            padding: 10px;
            margin: 10px 0;
        }

        .review p {
            margin: 0;
        }

        .hide {
            display: none;
        }

        .fill1 {
            position: absolute;
            background-color: #26a541;
            animation: fill-1 5s forwards, colors 5s forwards;
            animation-iteration-count: 1;
        }

        .fill2 {
            position: absolute;
            background-color: #26a541;
            animation: fill-1 5s forwards;
            animation-iteration-count: 1;
            animation-delay: 4s;
            visibility: hidden;
        }

        .fill3 {
            width: 0;
            position: absolute;
            background-color: #26a541;
            animation: fill-1 5s forwards;
            animation-iteration-count: 1;
            animation-delay: 8s;
            visibility: hidden;
        }
        .fill4 {
            width: 0;
            position: absolute;
            background-color: #26a541;
            animation: fill-2 2s forwards;
            animation-iteration-count: 1;
       
            visibility: hidden;
        }
        .step.active-cancel {
            background-color: rgb(228, 49, 49);
          
            border-color: rgb(228, 49, 49);
        }
        .step-cancel {
            width: 20px;
            height: 20px;
            border: 2px solid #ddd;
            border-radius: 50%;
            display: flex;
            align-items: center;
            justify-content: center;
            font-size: 16px;

        }

        @keyframes fill-1 {
            0% {
                width: 0;
                visibility: visible;
            }


            100% {
                width: 110px;
                visibility: visible;
            }
        }
        @keyframes fill-2 {
            0% {
                width: 0;
                visibility: visible;
            }


            100% {
                width: 356px;
                visibility: visible;
            }
        }
    </style>
