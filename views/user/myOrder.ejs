        <div class="col-md-9">
          <div class="d-flex flex-column">
            <div class="div" style="width: 85%; background-color: #fff">
              <div class="input-group">
                <input
                  type="text"
                  class="form-control"
                  placeholder="Recipient's username"
                  aria-label="Recipient's username"
                  aria-describedby="button-addon2"
                />
                <button class="btn btn-dark" type="button" id="button-addon2">
                  Search orders
                </button>
              </div>
            </div>

            <div class="card-body mt-4">
              <div class="table-responsive">
                <table class="table table-hover">
                  <thead>
                    <tr>
                      <th>Index</th>
                      <th scope="col">Name</th>
                      <th scope="col">Email</th>
                      <th scope="col">Total</th>
                      <th scope="col">Status</th>
                      <th scope="col">Date</th>
                      <th scope="col">Action</th>
                    </tr>
                  </thead>
                  <tbody>
                    <%if(order) {%> <%order.forEach((item, i) => {%>

                    <tr style="margin-bottom: 2rem">
                      <td style="padding: 24px"><%= i + 1%></td>
                      <td style="padding: 24px"><b><%= item.user.name%></b></td>
                      <td style="padding: 24px"><%= item.user.name%></td>
                      <td style="padding: 24px"><%= item.totalAmount%></td>
                      <td style="padding: 24px">
                        <span class="badge rounded-pill alert-warning"
                          ><%= item.status%></span
                        >
                      </td>
                      <% const formattedDate = item.date.toLocaleString('en-US',
                      { weekday: 'short', month: 'short', day: 'numeric', year:
                      'numeric', hour: 'numeric', minute: 'numeric', second:
                      'numeric', hour12: true }); %>
                      <td style="padding: 24px"><%= formattedDate%></td>
                      <td style="padding: 24px">
                        <a href="/single-orderDetails?orderId=<%= item._id%>"
                          >More details</a
                        >
                        <!-- dropdown //end -->
                      </td>
                    </tr>

                    <%})%> <%} else {%> <%}%>
                  </tbody>
                </table>
              </div>
              <!-- table-responsive //end -->
            </div>
          </div>
          <div class="pagination-area mt-15 mb-50">
            <nav aria-label="Page navigation example">
              <ul class="pagination">
                <%if(page != 0) {%>
                <a class="page-link btn btn-dark" href="?page=<%= page - 1%>"
                  ><i class="material-icons md-chevron_left">Previous</i></a
                >
                <%}%> <% for (let i = 0; i < orderLength; i++) { %> <% if (i ===
                0 || i === orderLength - 1 || (i >= page - 2 && i <= page + 2))
                { %>
                <li
                  class="btn btn-outline-dark <%= page == i ? 'active' : '' %>"
                >
                  <a class="" href="?page=<%= i %>"><%= i + 1 %></a>
                </li>
                <% } else if (i === page - 3 || i === page + 3) { %>
                <li class="page-item disabled">
                  <span class="page-link">...</span>
                </li>
                <% } %> <% } %> <%if(page != orderLength) {%>
                <a class="p btn btn-dark" href="?page=<%= page + 1%>"
                  ><i class="material-icons md-chevron_right">Next</i></a
                >
                <%}%>
              </ul>
            </nav>
          </div>
